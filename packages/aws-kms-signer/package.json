    "secp256k1": "4.0"
  },
  "devDependencies": {
    "@types/asn1js": "^2.0.2",
    "@types/bn.js": "^5.1.0",
    "@types/keccak": "^3.0.1",
    "@types/secp256k1": "^4.0.3"
  },
  "publishConfig": {
    "access": "public"
