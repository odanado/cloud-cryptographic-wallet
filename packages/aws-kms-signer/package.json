{
  "name": "aws-kms-signer",
  "version": "0.4.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/odanado/aws-kms-provider"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "node": "./dist/index.cjs",
      "default": "./dist/index.js"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
